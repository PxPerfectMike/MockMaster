{
  "name": "@mockmaster/openapi",
  "version": "0.0.0",
  "description": "OpenAPI parsing and type generation for mock-master",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "openapi",
    "swagger",
    "mock"
  ],
  "license": "MIT",
  "dependencies": {
    "@mockmaster/core": "workspace:*",
    "fp-ts": "^2.16.2",
    "openapi-types": "^12.1.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@vitest/coverage-v8": "^1.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
